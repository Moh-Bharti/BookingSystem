						String[] we=er.split(",");
					}
					while((er=ob.readLine())!=null){
						String[]ger=er.split(Sp);
					}
					
				}catch(FileNotFoundException e){
					
				}catch(IOException f){
					throw f;
				}
		
		
		List<Course> r=new ArrayList<Course>();
