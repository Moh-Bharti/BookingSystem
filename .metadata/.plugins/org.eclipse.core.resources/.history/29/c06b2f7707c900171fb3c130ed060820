						v[2][8].add(a);
					}
					else if(a.timeslot.containsKey("1:30")&&a.lecture==1.5){
						v[2][10].add(a);
						v[2][11].add(a);
						v[2][12].add(a);
					}
					else if(a.timeslot.containsKey("3:00")&&a.lecture==1){
						v[2][13].add(a);
						v[2][14].add(a);
					}else if(a.timeslot.containsKey("4:00")&&a.lecture==1.5){
						v[2][15].add(a);
						v[2][16].add(a);
						v[2][17].add(a);
					}
					
					
				}
				else if(a.timeslot.containsKey("Thursday")){
					if(a.timeslot.containsValue("8:30")&&a.lecture==1.5){
						v[3][0].add(a);
						v[3][1].add(a);
						v[3][2].add(a);
					}
					else if(a.timeslot.containsKey("10:00")&&a.lecture==1){
						v[3][3].add(a);
						v[3][4].add(a);
					}
					else if(a.timeslot.containsKey("11:00")&&a.lecture==1){
						v[3][5].add(a);
						v[3][6].add(a);
					}else if(a.timeslot.containsKey("11:30")&&a.lecture==1.5){
						v[3][6].add(a);
						v[3][7].add(a);
						v[3][8].add(a);
					}
					else if(a.timeslot.containsKey("1:30")&&a.lecture==1.5){
						v[3][10].add(a);
						v[3][11].add(a);
						v[3][12].add(a);
					}
					else if(a.timeslot.containsKey("3:00")&&a.lecture==1){
						v[3][13].add(a);
						v[3][14].add(a);
					}else if(a.timeslot.containsKey("4:00")&&a.lecture==1.5){
						v[3][15].add(a);
						v[3][16].add(a);
						v[3][17].add(a);
					}
					
					
				}else{
					if(a.timeslot.containsValue("8:30")&&a.lecture==1.5){
						v[4][0].add(a);
						v[4][1].add(a);
						v[4][2].add(a);
					}
					else if(a.timeslot.containsKey("10:00")&&a.lecture==1){
						v[3][3].add(a);
						v[3][4].add(a);
					}
					else if(a.timeslot.containsKey("11:00")&&a.lecture==1){
						v[0][5].add(a);
						v[0][6].add(a);
					}else if(a.timeslot.containsKey("11:30")&&a.lecture==1.5){
						v[0][6].add(a);
						v[0][7].add(a);
						v[0][8].add(a);
					}
					else if(a.timeslot.containsKey("1:30")&&a.lecture==1.5){
						v[0][10].add(a);
						v[0][11].add(a);
						v[0][12].add(a);
					}
					else if(a.timeslot.containsKey("3:00")&&a.lecture==1){
						v[0][13].add(a);
						v[0][14].add(a);
					}else if(a.timeslot.containsKey("4:00")&&a.lecture==1.5){
						v[0][15].add(a);
						v[0][16].add(a);
						v[0][17].add(a);
